{
	"$schema": "http://json.schemastore.org/template",
	"author": "Fluxera",
	"classifications": [
		"Common",
		"Service",
		"Host"
	],
	"name": "Service Host",
	"description": "A project for creating a service host that targets .NET",
	"groupIdentity": "Fluxera.Common.Service.Host",
	"precedence": "9000",
	"identity": "Fluxera.Common.Service.Host.CSharp",
	"shortName": "servicehost",
	"tags": {
		"language": "C#",
		"type": "project"
	},
	"sourceName": "ServiceHost1",
	"preferNameDirectory": true,
	"defaultName": "Service1",
	"symbols": {
		"Framework": {
			"type": "parameter",
			"description": "The target framework for the module.",
			"datatype": "choice",
			"choices": [
				{
					"choice": "net6.0",
					"description": "Target .NET 6.0"
				}
			],
			"replaces": "net6.0",
			"defaultValue": "net6.0"
		},
		"HostIdentifier": {
			"type": "bind",
			"binding": "HostIdentifier"
		}
	},
	"postActions": [
		{
			"id": "restore",
			"condition": "(!skipRestore)",
			"description": "Restore NuGet packages required by this project.",
			"manualInstructions": [
				{
					"text": "Run 'dotnet restore'"
				}
			],
			"actionId": "210D431B-A78B-4D2F-B762-4ED3E3EA9025",
			"continueOnError": true
		}
	]
}